		{
			"name": "Register",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"if (pm.response.to.have.status(200)) {\r",
							"    var user_id = pm.environment.get(\"LAST_CREATED_USER_ID\");\r",
							"\r",
							"    if (user_id != null) {\r",
							"        pm.environment.set(\"LAST_CREATED_USER_ID\", user_id+1);\r",
							"    } else {\r",
							"        pm.environment.set(\"LAST_CREATED_USER_ID\", 1);\r",
							"    }\r",
							"}"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"auth": {
					"type": "noauth"
				},
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"firstName\": \"John\",\r\n    \"lastName\": \"Doe\",\r\n    \"email\": \"test@gmail.com\",\r\n    \"password\": \"12345678\",\r\n    \"mfaEnabled\": true\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{env}}/api/v1/auth/register",
					"host": [
						"{{env}}"
					],
					"path": [
						"api",
						"v1",
						"auth",
						"register"
					]
				}
			},
			"response": []
		},
		{
			"name": "Verify Account",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"if (pm.response.to.have.status(200)) {\r",
							"    const responseJson = pm.response.json();\r",
							"\r",
							"    var accessToken = responseJson.accessToken;\r",
							"    pm.environment.set(\"ACCESS_TOKEN\", accessToken);\r",
							"\r",
							"    var refreshToken = responseJson.refreshToken;\r",
							"    pm.environment.set(\"REFRESH_TOKEN\", refreshToken);\r",
							"}"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"protocolProfileBehavior": {
				"disableBodyPruning": true
			},
			"request": {
				"auth": {
					"type": "noauth"
				},
				"method": "GET",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{env}}/api/v1/auth/{{LAST_CREATED_USER_ID}}/activate-account?token=841086",
					"host": [
						"{{env}}"
					],
					"path": [
						"api",
						"v1",
						"auth",
						"{{LAST_CREATED_USER_ID}}",
						"activate-account"
					],
					"query": [
						{
							"key": "token",
							"value": "841086"
						}
					]
				}
			},
			"response": []
